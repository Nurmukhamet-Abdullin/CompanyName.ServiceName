@model IReadOnlyList<CompanyName.ServiceName.Contracts.DataAccess.CalculationMeteringDevice>
@{
    ViewData["Title"] = "Calculation Devices";
}

@if (Model.Count() == 0)
{
    <div class="row">
        <div class="col text-center p-2">No Data</div>
    </div>
}
else
{
    <table class="table">
        <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model[0].Id)</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Id)</td>
            </tr>
        }
        </tbody>
    </table>
}
